{
  "name": "n-expo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "clean": "expo prebuild --clean",
    "build:android": "eas build --platform android --profile production",
    "build:android-local": "eas build --platform android --profile production --local",
    "workflow:run": "npx eas-cli workflow:run",
    "workflow:android": "npx eas-cli workflow:run .eas/workflows/android-build.yml",
    "predeploy": "expo export",
    "deploy": "gh-pages -d dist",
    "full-deploy": "npm run predeploy && npm run deploy"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.12",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.3.1",
    "axios": "^1.9.0",
    "expo": "~53.0.9",
    "expo-status-bar": "~2.2.3",
    "firebase": "^11.7.1",
    "moment": "^2.30.1",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-paper": "^5.14.1",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-vector-icons": "^10.2.0",
    "expo-dev-client": "~5.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
